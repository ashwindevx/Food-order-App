n  const hasItems = cartCtx.items.length > 0;\n\n  const cartItemRemoveHandler = (id) => {\n    cartCtx.removeItem(id);\n  };\n\n  const cartItemAddHandler = (item) => {\n    cartCtx.addItem(item);\n  };\n\n  const submitOrderHandler = async (userData) => {\n    setIsSubmitting(true);\n    const response = await fetch(\n      \"https://react-http-post-5fa60-default-rtdb.firebaseio.com/orders.json\",\n      {\n        method: \"POST\",\n        body: JSON.stringify({\n          user: userData,\n          orderedItems: cartCtx.items,\n        }),\n      }\n    );\n    setIsSubmitting(false);\n    setDidSubmit(true);\n    cartCtx.clearCart();\n  };\n\n  const cartItems = (\n    <ul className={classes[\"cart-items\"]}>\n      {cartCtx.items.map((item) => (\n        <CartItem\n          key={item.id}\n          name={item.name}\n          amount={item.amount}\n          price={item.price}\n          onRemove={cartItemRemoveHandler.bind(null, item.id)}\n          onAdd={cartItemAddHandler.bind(null, item)}\n        />\n      ))}\n    </ul>\n  );\n\n  const orderHandler = () => {\n    setIsCheckout(true);\n  };\n\n  const cardModalContent = (\n    <>\n      {cartItems}\n      <div className={classes.total}>\n        <span>Total Amount</span>\n        <span>{totalAmount}</span>\n      </div>\n      {isCheckout ? (\n        <Checkout onConfirm={submitOrderHandler} onCancel={props.onClose} />\n      ) : (\n        <div className={classes.actions}>\n          <button className={classes[\"button--alt\"]} onClick={props.onClose}>\n            Close\n          </button>\n          {hasItems && (\n            <button className={classes.button} onClick={orderHandler}>\n              Order\n            </button>\n          )}\n        </div>\n      )}\n    </>\n  );\n\n  const isSubmittingContent = <p>Sending the data...</p>;\n\n  const didSubmitData = (\n    <>\n      <p>Successfully sent the order</p>\n      <div className={classes.actions}>\n        <button className={classes.button} onClick={props.onClose}>\n          Close\n        </button>\n      </div>\n    </>\n  );\n\n  return (\n    <Modal onClose={props.onClose}>\n      {!isSubmitting && !didSubmit && cardModalContent}\n      {isSubmitting && isSubmittingContent}\n      {!isSubmitting && didSubmit && didSubmitData}\n    </Modal>\n  );\n};\n\nexport default Cart;\n","/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Layout/Header.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Meals/Meals.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/store/cart-context.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Meals/MealsSummary.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Cart/CartItem.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Layout/HeaderCartButton.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Cart/Checkout.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Meals/AvailableMeals.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/UI/Modal.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/UI/Card.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Cart/CartIcon.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Meals/MealItem/MealItem.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/Meals/MealItem/MealItemForm.js",[],"/Users/ashwinsmacbook/Desktop/food-order-app/src/components/UI/Input.js",[],{"ruleId":"76","severity":1,"message":"77","line":28,"column":11,"nodeType":"78","messageId":"79","endLine":28,"endColumn":19},"no-unused-vars","'response' is assigned a value but never used.","Identifier","unusedVar"]